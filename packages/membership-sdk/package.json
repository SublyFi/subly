{
  "name": "@subly/membership-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Subly Membership Protocol (Protocol A)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.0",
    "@solana/web3.js": "^1.98.0",
    "bs58": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "~5.7.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": "^0.32.0",
    "@solana/web3.js": "^1.98.0"
  },
  "keywords": [
    "solana",
    "subscription",
    "membership",
    "privacy",
    "arcium"
  ],
  "author": "Subly Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/subly-protocol/subly-devnet"
  }
}
